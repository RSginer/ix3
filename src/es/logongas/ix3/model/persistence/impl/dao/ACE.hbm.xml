<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="es.logongas.ix3.model.ACE" >
        <id name="idACE" >
            <generator class="identity" />
        </id>
        <property name="aceType" >
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">es.logongas.ix3.model.ACEType</param>
                <param name="type">12</param>
            </type>
        </property>
        <many-to-one name="permission">
            <column name="idPermission"  />
        </many-to-one>
        <many-to-one name="identity">
            <column name="idIdentity"  />
        </many-to-one>
        <property name="secureResourceRegExp"  />
        <property name="conditionalScript"  />
        <property name="priority"  />
    </class>
</hibernate-mapping>